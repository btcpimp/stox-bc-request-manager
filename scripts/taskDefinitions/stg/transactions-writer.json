      "environment": [
        {
          "name": "DATABASE_URL",
          "value": "postgres://stgrmuser:NvDdaV48NBkAjb584F4UuVsP6PQ9M5@stg-request-manager.cychkzdvrrri.eu-west-1.rds.amazonaws.com:5432/stoxbcrm"
        },
        {
